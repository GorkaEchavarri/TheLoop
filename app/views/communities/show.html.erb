<div class="container">
  <h1><%= @community.title %></h1>
  <p><%= @community.description %></p>
  <%= link_to "Edit",
    edit_community_path(@community)
  %>
  <%= link_to "Delete",
    community_path(@community),
    data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
  %>
  <br>
  <%= link_to "All communities",
    communities_path
  %>
</div>
